{
    "feature_groups": {
        "price_cols": [
            "SE3_price_ore",
            "price_24h_avg",
            "price_168h_avg",
            "price_24h_std",
            "hour_avg_price",
            "price_vs_hour_avg"
        ],
        "grid_cols": [
            "fossilFreePercentage",
            "renewablePercentage",
            "powerConsumptionTotal",
            "powerProductionTotal",
            "powerImportTotal",
            "powerExportTotal",
            "nuclear",
            "wind",
            "hydro",
            "solar",
            "unknown",
            "import_SE-SE2",
            "export_SE-SE4",
            "import_NO-NO1",
            "export_NO-NO1",
            "import_DK-DK1",
            "export_DK-DK1",
            "import_FI",
            "export_FI",
            "import_AX",
            "export_AX"
        ],
        "cyclical_cols": [
            "hour_sin",
            "hour_cos",
            "day_of_week_sin",
            "day_of_week_cos",
            "month_sin",
            "month_cos"
        ],
        "binary_cols": [
            "is_peak_hour",
            "is_weekend",
            "is_holiday",
            "is_holiday_eve",
            "season"
        ]
    },
    "feature_metadata": {
        "target_feature": "SE3_price_ore",
        "feature_order": [
            "price_cols",
            "cyclical_cols",
            "binary_cols",
            "grid_cols"
        ],
        "description": {
            "price_cols": "Price-related features including target variable",
            "grid_cols": "Grid supply and generation features",
            "cyclical_cols": "Time-based cyclical features",
            "binary_cols": "Binary and categorical features including holidays"
        }
    },
    "model_config": {
        "architecture": {
            "lstm_layers": [
                {
                    "units": 128,
                    "return_sequences": true,
                    "dropout": 0.1
                },
                {
                    "units": 96,
                    "return_sequences": true,
                    "dropout": 0.1
                },
                {
                    "units": 64,
                    "return_sequences": false,
                    "dropout": 0.1
                }
            ],
            "dense_layers": [
                {
                    "units": 96,
                    "activation": "relu"
                },
                {
                    "units": 48,
                    "activation": "relu"
                },
                {
                    "units": 24,
                    "activation": null
                }
            ],
            "output_units": 24
        },
        "training": {
            "window_size": 168,
            "prediction_horizon": 24,
            "batch_size": 32,
            "max_epochs": 150,
            "learning_rate": 0.001,
            "optimizer": "adam",
            "loss": "huber",
            "metrics": [
                "mae"
            ]
        },
        "callbacks": {
            "early_stopping": {
                "monitor": "val_loss",
                "patience": 15,
                "restore_best_weights": true
            },
            "reduce_lr": {
                "monitor": "val_loss",
                "factor": 0.2,
                "patience": 10,
                "min_lr": 1e-06
            }
        },
        "data_split": {
            "train_ratio": 0.9,
            "val_ratio": 0.05,
            "test_ratio": 0.05
        },
        "scaling": {
            "price_scaler": {
                "type": "RobustScaler",
                "quantile_range": [
                    1,
                    99
                ],
                "unit_variance": false,
                "clip_negative": false,
                "max_reasonable_price": 1000
            },
            "price_transform_config": {
                "enable_log_transform": true,
                "apply_to_cols": [
                    "SE3_price_ore"
                ],
                "offset": 1.0,
                "scaling_correction_factor": false
            },
            "grid_scaler": {
                "type": "RobustScaler",
                "quantile_range": [
                    1,
                    99
                ],
                "unit_variance": false,
                "handle_extreme_values": true,
                "outlier_threshold": 8,
                "max_zscore": 8,
                "import_export_cols": [
                    "powerImportTotal",
                    "powerExportTotal",
                    "import_SE-SE2",
                    "export_SE-SE4",
                    "import_NO-NO1",
                    "export_NO-NO1",
                    "import_DK-DK1",
                    "export_DK-DK1",
                    "import_FI",
                    "export_FI",
                    "import_AX",
                    "export_AX"
                ],
                "log_transform_large_values": true,
                "individual_scaling": true
            }
        }
    }
}